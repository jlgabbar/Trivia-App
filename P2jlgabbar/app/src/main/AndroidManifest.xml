<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.jlgabbarolemiss.p2jlgabbar" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".math"
            android:label="@string/app_name" >
            <intent-filter>
                 <action android:name="edu.jlgabbarolemiss.p2jlgabbar.math" />
                 <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

            </activity>

        <activity android:name=".geography" android:label="@string/app_name">
            <intent-filter>
                <action android:name="edu.jlgabbarolemiss.p2jlgabbar.geography"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>


        </activity>


    </application>

</manifest>
